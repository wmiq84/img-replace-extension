{
    "manifest_version": 3,
    "name": "Replace With Markiplier",
    "version": "1.0",
    "description": "Replaces all images on a page with Markiplier's face.",
    "permissions": [],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["markiplier.jpg"],
        "matches": ["<all_urls>"]
      }
    ]
}
  